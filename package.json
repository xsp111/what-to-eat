{
	"name": "what-to-eat",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite --host",
		"dev:server": "concurrently \"esbuild ./server/app.js --watch --platform=node --bundle --outfile=./server/dist/dev.cjs\" \"wait-on server/dist/dev.cjs && node --watch server/dist/dev.cjs\"",
		"build": "vite build",
		"preview": "vite preview",
		"server:build": "node esbuild.config.cjs",
		"server:start": "node server/dist/index.cjs",
		"update:mgDB": "npx prisma db push --schema=./server/db/prisma/schema.prisma",
		"update:prisma:config": "npx prisma generate --schema=./server/db/prisma/schema.prisma",
		"lint": "eslint ."
	},
	"dependencies": {
		"@ant-design/icons": "^5.6.1",
		"@langchain/core": "^1.0.5",
		"@langchain/deepseek": "^1.0.1",
		"@prisma/client": "^6.18.0",
		"@react-three/drei": "^10.7.7",
		"@react-three/fiber": "^9.4.2",
		"@tailwindcss/vite": "^4.1.16",
		"antd": "^5.27.6",
		"cookie-parser": "^1.4.7",
		"dayjs": "^1.11.19",
		"dotenv": "^17.2.3",
		"esbuild": "^0.25.11",
		"express": "^5.1.0",
		"langchain": "^1.0.4",
		"multer": "^2.0.2",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-markdown": "^10.1.0",
		"react-router": "^7.9.4",
		"recharts": "^3.5.1",
		"rehype-sanitize": "^6.0.0",
		"remark-gfm": "^4.0.1",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.16",
		"three": "^0.181.2",
		"zustand": "5.0.8"
	},
	"devDependencies": {
		"vite-plugin-pwa": "^1.1.0",
		"@eslint/js": "^9.36.0",
		"@types/react": "^19.1.16",
		"@types/react-dom": "^19.1.9",
		"@vitejs/plugin-react": "^5.0.4",
		"concurrently": "^9.2.1",
		"eslint": "^9.36.0",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.22",
		"globals": "^16.4.0",
		"prisma": "^6.18.0",
		"vite": "^7.1.7",
		"wait-on": "^9.0.3"
	}
